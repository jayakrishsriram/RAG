{
    "version": 2,
    "builds": [
      {
        "src": "fastapi/app_fastapi.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/app.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "fastapi/app_fastapi.py"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/index.html"
      }
    ]
  }
  